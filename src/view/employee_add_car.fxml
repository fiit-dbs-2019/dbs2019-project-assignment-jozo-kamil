<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="600.0" prefWidth="600.0" stylesheets="@../our.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EmployeeAddCarController">
    <children>
        <HBox alignment="CENTER" prefHeight="60.0" prefWidth="600.0">
            <children>
                <Label alignment="CENTER" prefHeight="85.0" prefWidth="442.0" text="Pridanie záznamu o aute">
                    <font>
                        <Font name="Lucida Console" size="30.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox layoutX="74.0" layoutY="60.0" prefHeight="460.0" prefWidth="380.0" spacing="40.0">
            <children>
                <VBox prefHeight="460.0" prefWidth="145.0" spacing="10.0">
                    <children>
                        <Label prefHeight="40.0" prefWidth="120.0" text="Značka:" />
                        <Label prefHeight="40.0" prefWidth="120.0" text="Model:" />
                        <Label prefHeight="40.0" prefWidth="160.0" text="Rok výroby:" />
                  <Label prefHeight="33.0" prefWidth="160.0" text="Stav tachometra:" />
                        <Label prefHeight="40.0" prefWidth="120.0" text="Palivo:" />
                        <Label prefHeight="40.0" prefWidth="120.0" text="Objem motora:" />
                        <Label prefHeight="40.0" prefWidth="120.0" text="Výkon motora:" />
                        <Label prefHeight="40.0" prefWidth="160.0" text="Typ prevodovky:" />
                        <Label prefHeight="40.0" prefWidth="120.0" text="Karoséria:" />
                        <Label prefHeight="40.0" prefWidth="120.0" text="Farba:" />
                  <Label prefHeight="40.0" prefWidth="120.0" text="ŠPZ:" />
                  <Label prefHeight="40.0" prefWidth="120.0" text="VIN:" />
                  <Label prefHeight="17.0" prefWidth="141.0" text="Cena výpožičky/deň:" />
                    </children>
                </VBox>
                <VBox prefHeight="460.0" prefWidth="187.0" spacing="10.0">
                    <children>
                  <ComboBox fx:id="comboBoxBrand" prefHeight="35.0" prefWidth="185.0" />
                  <ComboBox fx:id="comboBoxModel" prefHeight="35.0" prefWidth="185.0" />
                        <DatePicker fx:id="datePickerYear" prefHeight="35.0" prefWidth="185.0" />
                  <Spinner fx:id="spinnerMileage" prefHeight="35.0" prefWidth="185.0" />
                  <ComboBox fx:id="comboBoxFuel" prefHeight="35.0" prefWidth="185.0" />
                         <Spinner fx:id="spinnerEngineCapacity" prefHeight="35.0" prefWidth="185.0" />
                         <Spinner fx:id="spinnerEnginePower" prefHeight="35.0" prefWidth="185.0" />
                  <ComboBox fx:id="comboBoxGearBox" prefHeight="35.0" prefWidth="185.0" />
                  <ComboBox fx:id="comboBoxCarBody" prefHeight="35.0" prefWidth="185.0" />
                  <ComboBox fx:id="comboBocColor" prefHeight="35.0" prefWidth="185.0" />
                  <TextField fx:id="textFieldSpz" prefHeight="35.0" prefWidth="185.0" />
                  <TextField fx:id="textFieldVin" prefHeight="35.0" prefWidth="185.0" />
                  <Spinner fx:id="spinnerPrice" prefHeight="35.0" prefWidth="185.0" />
                    </children>
                </VBox>
            </children>
        </HBox>
        <VBox layoutX="466.0" layoutY="499.0" prefHeight="78.0" prefWidth="120.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="20.0" prefWidth="102.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#btnBackPushed" text="Späť" />
                    </children>
                </HBox>
                <HBox alignment="BOTTOM_CENTER" prefHeight="74.0" prefWidth="128.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#btnConfirmPushed" prefHeight="38.0" prefWidth="120.0" text="Pridať" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
